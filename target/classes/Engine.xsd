<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="Engines">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Engine" maxOccurs="unbounded" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:byte" name="engine_id"/>
							<xs:element type="xs:string" name="name"/>
							<xs:element type="xs:short" name="horsepower"/>
							<xs:element type="xs:byte" name="consumption"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>